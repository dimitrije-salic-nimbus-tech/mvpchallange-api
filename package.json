{
  "name": "mvpmatch",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=dev ts-node src/index.ts",
    "start:dev": "NODE_ENV=dev ts-node-dev src/index.ts",
    "build": "tsc",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:run": "NODE_ENV=dev yarn run typeorm migration:run -c mvpmatch",
    "migration:generate": "NODE_ENV=dev yarn run typeorm migration:generate -c mvpmatch",
    "migration:revert": "NODE_ENV=dev yarn run typeorm migration:revert -c mvpmatch",
    "migration:create": "NODE_ENV=dev yarn run typeorm migration:create -c mvpmatch",
    "test:integration": "NODE_ENV=test jest --config=__integration-test__/jest.config.js --testRunner='jest-circus/runner' --ci --verbose --maxWorkers=1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/jest": "^26.0.24",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.6.2",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.8.0",
    "ts-node-dev": "^2.0.0-0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "amazon-cognito-identity-js": "^5.2.9",
    "celebrate": "^15.0.1",
    "class-transformer": "0.4.0",
    "cognito-express": "^3.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "pg": "^8.7.3",
    "redis": "^3.1.2",
    "typeorm": "0.2.36"
  }
}
